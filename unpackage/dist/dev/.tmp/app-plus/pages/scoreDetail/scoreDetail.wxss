.scroll-view_H {
  white-space: nowrap;
}
.scroll-view_H .scroll-view-item_H {
    display: inline-block;
    width: 100px;
    height: 90px;
    margin: 10px 0 10px 10px;
}
.scroll-view_H .scroll-view-item_H image {
      width: 100%;
      height: 100%;
}
.SD_Info {
  font-size: 12px;
  padding: 0 10px;
}
.SD_Info view {
    color: #b7b7b7;
    margin-top: 15px;
}
.SD_Info view text {
      color: #333333;
      margin-left: 20px;
}
.SD_Tag {
  padding: 10px;
}
.SD_Tag text {
    font-size: 14px;
    color: #5f5f5f;
    font-weight: 700;
}
.SD_Tag view {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    margin-top: 8px;
}
.SD_Tag view text {
      border: 1px solid #ececec;
      padding: 4px 9px;
      font-weight: normal;
      font-size: 12px;
      border-radius: 5px;
      margin-left: 10px;
}
.SD_Tag view text:first-child {
        margin-left: 0;
}
.SD_Remark {
  padding: 10px;
}
.SD_Remark text {
    font-size: 14px;
    color: #5f5f5f;
    font-weight: 700;
}
.SD_Remark textarea {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    margin-top: 8px;
    text-indent: 10px;
    color: #333333;
    font-size: 12px;
    border: 1px solid #ececec;
}
.SD_Detail {
  padding: 10px 0;
}
.SD_Detail .SD_Detail_Title {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    font-size: 14px;
    padding: 0 0 10px 10px;
    border-bottom: 1px solid #ececec;
}
.SD_Detail .SD_Detail_Cont {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
    padding: 0 10px;
}
.SD_Detail .SD_Detail_Cont view {
      width: 50%;
      margin-top: 10px;
      font-size: 12px;
      color: #b7b7b7;
}
.SD_Detail .SD_Detail_Cont view text {
        border: none;
        color: #5f5f5f;
}

